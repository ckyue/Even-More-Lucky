{
  "name": "Even More Lucky",
  "version": "0.0.1",
  "manifest_version": 2,
  "description": "Enhance your searching experience",
  "homepage_url": "http://cky.im",
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "default_locale": "en",
  "options_page": "src/options_custom/options.html",
  "permissions": [
    "storage"
  ],
  "content_scripts": [
    {
      "matches": ["*://www.google.ca/*","*://www.google.com/*","*://www.google.hk/*"],
      "css": ["src/inject/inject.css"],
      "js": ["src/inject/inject.js", "src/inject/jquery-2.1.1.min.js"]
    }
 ]
}
